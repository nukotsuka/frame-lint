{
  "name": "frame-lint",
  "version": "1.0.0",
  "description": "Find and fix invalid frame names in Figma",
  "private": true,
  "scripts": {
    "build": "pnpm run build:plugin && pnpm run build:ui && pnpm run build:message-types",
    "build:plugin": "pnpm --filter @frame-lint/plugin build",
    "build:ui": "pnpm --filter @frame-lint/ui build",
    "build:message-types": "pnpm --filter @frame-lint/message-types build",
    "dev": "pnpm run dev:plugin & pnpm run dev:ui & pnpm run dev:message-types",
    "dev:plugin": "pnpm --filter @frame-lint/plugin dev",
    "dev:ui": "pnpm --filter @frame-lint/ui dev",
    "dev:message-types": "pnpm --filter @frame-lint/message-types dev",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "author": "nukotsuka",
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@figma/plugin-typings": "*",
    "eslint": "^9.33.0",
    "eslint-plugin-chakra-ui": "^0.12.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jiti": "^2.5.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1"
  },
  "packageManager": "pnpm@10.14.0",
  "volta": {
    "node": "24.6.0",
    "pnpm": "10.14.0"
  }
}
